<script type="text/JavaScript">  
    function timer(){  
        var time = new Date();  
        document.getElementById("currentTime").innerHTML="当前时间：" + time;  
        //document.write("当前时间：" + time); //因为页面内容被覆盖，所以页面显示当前时间后不会更新  
    }  
    setInterval("timer()", 1000);  
</script> 

